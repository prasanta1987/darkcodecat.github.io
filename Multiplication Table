<body>
<script type="text/javascript">
var n;
var ans;

n =prompt("Enter any number");
document.write("Multiplication Table of ",+n," is: ");
 for (i=1;i<=20;i++){
 ans =n*i;
 document.write("<br>" +n+ "*" +i+" = ",+ans);
 }
</script>
